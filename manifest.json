{
  "name": "inDoors - Show Glassdoor ratings on LinkedIn",
  "short_name": "inDoors",
  "description": "Shows Glassdoor ratings for companies when searching on Linkedin",
  "version": "3.3.0",

  "content_scripts": 
    [
        {
            "matches":["https://*.linkedin.com/*"],
            "css":["linker.css"],
            "js":["lib/arrive.min.js",  "dist/utils.min.js", "dist/misdirected-names.min.js", "src/linker.js"],
            "run_at": "document_end"
        }
    ],
  "permissions": [
    "storage",
    "https://glassdoor-search.netlify.app/.netlify/functions/gdinfo?company=*"
  ],
  "icons": {
    "16":"icon/icon16.png",
    "48":"icon/icon48.png",
    "128":"icon/icon128.png" 
  },
  "manifest_version": 2
}