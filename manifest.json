{
  "name": "inDoors - Glassdoor Integrator for Linkedin",
  "short_name": "inDoors",
  "description": "Shows Glassdoor reviews for companies when searching on Linkedin",
  "version": "3.1.1",

  "content_scripts": 
    [
        {
            "matches":["https://*.linkedin.com/*"],
            "css":["linker.css"],
            "js":["arrive.min.js", "misdirected-names.js", "build/linker.js"],
            "run_at": "document_end"
        }
    ],
  "permissions": [
    "storage",
    "https://glassdoor-search.netlify.app/.netlify/functions/gdinfo?company=*"
  ],
  "icons": {
    "16":"icon/icon16.png",
    "48":"icon/icon48.png",
    "128":"icon/icon128.png" 
  },
  "manifest_version": 2
}