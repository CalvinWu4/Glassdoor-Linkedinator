{
  "name": "inDoors - Glassdoor Integrator for Linkedin",
  "short_name": "inDoors",
  "description": "Shows Glassdoor reviews for companies when searching on Linkedin",
  "version": "2.1.0",

  "content_scripts": 
    [
        {
            "matches":["https://*.linkedin.com/jobs/search/*", "https://*.linkedin.com/my-items/saved-jobs/*"],
            "css":["linker.css"],
            "js":["arrive.min.js", "misdirected-names.js", "build/linker.js"],
            "run_at": "document_end"
        }
    ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16":"icon/icon16.png",
    "48":"icon/icon48.png",
    "128":"icon/icon128.png" 
  },
  "manifest_version": 2
}